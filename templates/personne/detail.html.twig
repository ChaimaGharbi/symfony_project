<link rel="stylesheet" href="{{ asset('node_modules/bootstrap/dist/css/bootstrap.css') }}">
<title>{{ personne.firstname }} {{ personne.name }}</title>
{% block body %}
<div class="row">
    <div class="card col-md-3 offset-md-1 my-5" style=" width: 18rem;">
        <div class="card-body">
            <h5 class="card-title">{{ personne.name }}</h5>
            <h6 class="card-subtitle mb-2 text-muted">{{ personne.firstname }} {{ personne.name }}</h6>
            <p class="card-text">Age : {{ personne.age }}</p>
            <a href="#" class="card-link">Job : {{ personne.job }}</a>
            <a href="#" class="card-link">{{ personne.id }}</a>
        </div>
    </div>
</div>
{% endblock %}

